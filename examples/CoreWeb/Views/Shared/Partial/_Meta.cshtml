@model Piranha.Models.IMeta

@if (!string.IsNullOrEmpty(Model.MetaKeywords)) {
<meta name="keywords" value="@Model.MetaKeywords">
}
@if (!string.IsNullOrEmpty(Model.MetaDescription)) {
    <meta name="description" value="@Model.MetaDescription">
}

    <meta name="og:title" content="@Model.Title">
    <meta name="og:type" content="article">
@if (!string.IsNullOrEmpty(Model.MetaDescription)) {
    <meta name="og:description" content="@Model.MetaDescription">
}